<script>
  // Get all elements with autoscroll attribute
  const autoscrollDivs = document.querySelectorAll('[autoscroll=true]');

  // Loop through each autoscroll div
  autoscrollDivs.forEach((div) => {
      // Get the first child element of the autoscroll div
      const child = div.firstElementChild;

      // Clone the child element
      const clonedChild = child.cloneNode(true);

      // Append the cloned child element as a sibling to the original child element
      child.parentNode.insertBefore(clonedChild, child.nextSibling);
  });

</script>

<!-- Floating Form Labels -->
<script>
$(document).ready(function() {
  // Function to check and update input states
  function checkInputStates() {
    $('.form-input').each(function() {
      if ($(this).val() !== '') {
        $(this).siblings('.field-label').addClass('float active-label');
      }
    });
  }

  // Check immediately
  checkInputStates();

  // Check again after a short delay to catch browser autofill
  setTimeout(checkInputStates, 100);

  // Also check on window load to be extra safe
  $(window).on('load', checkInputStates);

  // Existing focus handler
  $('.form-input').focus(function() {
    $(this).siblings('.field-label').addClass('float active-label');
  });

  // Existing blur handler
  $('.form-input').blur(function() {
    var inputVal = $(this).val();
    var label = $(this).siblings('.field-label');
    label.removeClass('active-label');
    if (!inputVal) {
      label.removeClass('float');
    }
  });
});
</script>

<!-- event triggers -->
<script type="text/javascript">
const trackEvent = (e, eventName, defaultAction) => {
  if (eventName !== undefined && eventName !== null && eventName !== '') {
    gtag("event", eventName, {
      event_category: e.attr("data-event-category") || "website_leads",
      event_location: e.attr("data-event-location"),
      event_action: e.attr("data-event-action") || defaultAction,
      value: e.attr("data-event-value"),
    });
    
    !!ko && ko.track(eventName, {
      action: e.attr("data-event-action") || defaultAction,
      location: e.attr("data-event-location")
    });
  }
};

// Attach an onClick listener to any element with the data-event-name custom property
$("[data-event-name]").click(function() {
  trackEvent($(this), $(this).attr("data-event-name"), "click");
});

// Track submissions of any form element
$("form").submit(function() {
  trackEvent($(this), $(this).attr("data-form-submission-event-name"), "submit");
});
</script>

