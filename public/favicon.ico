  'sbcglobal.net', 'sfr.fr', 'shaw.ca', 'sina.cn', 'sina.com', 'sky.com', 'skynet.be', 
  'speedy.com.ar', 'sympatico.ca', 't-online.de', 'talktalk.co.uk', 'telenet.be', 'teletu.it', 
  'terra.com.br', 'tin.it', 'tiscali.co.uk', 'tiscali.it', 'tuta.io', 'tutamail.com', 
  'tutanota.com', 'tutanota.de', 'tvcablenet.be', 'uol.com.br', 'verizon.net', 'virgilio.it', 
  'virgin.net', 'virginmedia.com', 'voo.be', 'wanadoo.fr', 'web.de', 'wow.com', 'ya.ru', 
  'yahoo.ca', 'yahoo.co.id', 'yahoo.co.in', 'yahoo.co.jp', 'yahoo.co.kr', 'yahoo.co.uk', 
  'yahoo.com', 'yahoo.com.ar', 'yahoo.com.br', 'yahoo.com.mx', 'yahoo.com.ph', 'yahoo.com.sg', 
  'yahoo.de', 'yahoo.fr', 'yahoo.it', 'yandex.by', 'yandex.com', 'yandex.kz', 'yandex.ru', 
  'yandex.ua', 'yeah.net', 'ygm.com', 'ymail.com', 'zipmail.com.br', 'zoho.com'
];

// Construct the regex pattern
const emailRegex = new RegExp(
  '^' +
  '([\\w.-]+@' +
  `(?!(?:${disallowedDomains.join('|')}|.*\\.edu))` + // blocks .edu here
  '[\\w-]+\\.)+' +
  '[\\w-]{2,4}' +
  '$',
  'i' // Case-insensitive flag
);

// Function to validate email
function validateEmail(email) {
  return emailRegex.test(email);
}

// Function to show error
function showError($element) {
  $element.addClass('is-invalid');
  $element.siblings('.form-error-label').addClass('is-active');
  document.querySelectorAll('.business-email').forEach((elt) => { elt.checked = false; })
}

// Function to hide error
function hideError($element) {
  $element.removeClass('is-invalid');
  $element.siblings('.form-error-label').removeClass('is-active');
  document.querySelectorAll('.business-email').forEach((elt) => { elt.checked = true; })
}

// Add event listener when the focus of the email field is changed
$('input[type="email"]').on('focusout', function() {
  var email = $(this).val();
  var gmailAllowed = $(this).attr('gmail') === 'allowed';

  if (!email) {
    // Show error if the field is empty
    showError($(this));
  } else if (!gmailAllowed && !validateEmail(email)) {
    // Log an event for analytics
    gtag("event", "trial_signup_personal_email_validation_error", {
      event_category: "website_leads",
      event_location: "trial_form",
      event_action: "focusout"
    });
    !!ko && ko.track("trial_signup_personal_email_validation_error");
    
    // Show error
    showError($(this));
  } else {
    // Hide error if email is valid or gmail is allowed
    hideError($(this));
  }
});

// Remove error classes as soon as the user starts typing
$('input[type="email"]').on('input', function() {
  hideError($(this));
});

// Remove error classes when the field gains focus
$('input[type="email"]').on('focusin', function() {
  hideError($(this));
});
});
</script>



<!-- dynamically change Favicon color based on browser theme -->
<script>
const lightFaviconUrl = 'https://cdn.prod.website-files.com/64d69beadfe5074323543ebd/6734e41b3fb9b25a198a0cca_Union.png';
const darkFaviconUrl = 'https://cdn.prod.website-files.com/64d69beadfe5074323543ebd/6734e4fd9d5ab3e0359ac223_favicon%20dark%20mode.png';

function setFavicon() {
const favicon = document.querySelector('link[rel="icon"]');
if (!favicon) {
  const newFavicon = document.createElement('link');
  newFavicon.rel = 'icon';
  document.head.appendChild(newFavicon);
}

const isDarkMode = window.matchMedia('(prefers-color-scheme: dark)').matches;
const faviconUrl = isDarkMode ? darkFaviconUrl : lightFaviconUrl;

document.querySelector('link[rel="icon"]').href = faviconUrl;
}

// Set favicon initially
setFavicon();

// Listen for changes in color scheme preference
window.matchMedia('(prefers-color-scheme: dark)').addEventListener('change', setFavicon);

</script>


<!-- Pass UTM paramaters through if user navigates multiple pages -->
<script>
document.addEventListener('DOMContentLoaded', function() {
  // Get the current URL's query parameters (UTM values)
  const queryParams = window.location.search;

  // Find all links on the page and append the UTM parameters to them
  const links = document.querySelectorAll('a:not([href="#"])');

  links.forEach(function(link) {
    if (link.href.includes('?')) {
      // If the link already has query parameters, just append the UTM params
      link.href += '&' + queryParams.substring(1);
    } else {
      // If the link has no query parameters, add the UTM params
      link.href += queryParams;
    }
  });
});
</script></body></html>